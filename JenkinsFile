pipeline {
    agent any
tools {
  maven 'maven3.6.3'
}
    stages {
        stage('Chekout') {
            steps {
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/ValaxyTech/simple-java-maven-app.git']])
            }
        }
        stage('Build') {
            steps {
                sh "mvn clean package"
            }
        }


    }
}

