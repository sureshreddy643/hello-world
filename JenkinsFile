pipeline {
    agent any
tools {
  maven 'maven'
}
    stages {
        stage('Chekout') {
            steps {
                checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/MithunTechnologiesDevOps/java-web-app-docker.git']])
            }
        }
        stage('Build') {
            steps {
                sh "mvn clean package"
            }
        }


    }
}

